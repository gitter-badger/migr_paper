@InProceedings{EPFL-CONF-191267,
   abstract    = {This paper investigates the performance of 6 versions of
                 Covariance Matrix Adaptation Evolution Strategy (CMAES)
                 with restarts on a set of 28 noiseless optimization
                 problems (including 23 multi-modal ones) designed for the
                 special session on real-parameter optimization of CEC
                 2013. The experimental validation of the restart
                 strategies shows that: i). the versions of CMA-ES with
                 weighted active covariance matrix update outperform the
                 original versions of CMA-ES, especially on illconditioned
                 problems; ii). the original restart strategies with
                 increasing population size (IPOP) are usually
                 outperformed by the bi-population restart strategies
                 where the initial mutation stepsize is also varied; iii).
                 the recently proposed alternative restart strategies for
                 CMA-ES demonstrate a competitive performance and are
                 ranked first w.r.t. the proportion of function-target
                 pairs solved after the full run on all 10-, 30- and
                 50-dimensional problems.},
   affiliation = {EPFL},
   author      = {Loshchilov, Ilya},
   booktitle   = {{IEEE} {C}ongress on {E}volutionary {C}omputation},
   details     = {http://infoscience.epfl.ch/record/191267},
   documenturl = {http://infoscience.epfl.ch/record/191267/files/CEC2013.pdf},
   oai-id      = {oai:infoscience.epfl.ch:191267},
   oai-set     = {conf},
   review      = {REVIEWED},
   status      = {PUBLISHED},
   submitter   = {230183; 148230},
   title       = {{CMA}-{ES} with {R}estarts for {S}olving {CEC} 2013
                 {B}enchmark {P}roblems},
   unit        = {IMT},
   year        = 2013
}

